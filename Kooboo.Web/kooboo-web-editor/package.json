  "main": "index.js",
  "scripts": {
    "start": "webpack --env.NODE_ENV=development",
    "build": "webpack --env.NODE_ENV=production",
    "lint": "eslint --fix src/**/*.ts tests/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "color-string": "^1.5.3",
    "query-string": "^6.7.0",
    "tinymce": "^5.0.6"
  },
  "devDependencies": {
    "@types/color-string": "^1.5.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "@types/tinymce": "^4.5.22",
    "@types/webpack": "^4.4.32",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.ts",
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "prettier": {
    "printWidth": 150
  }
}
